			function goBestLink(obj) {
				$(obj).siblings().removeClass("on");
				var dspCtgryNo = $(obj).attr('dspCtgryNo');
				var brndId = $(obj).attr('brndId');
				var otltYn = $(obj).attr('otltYn');
				var bestCd = "";
