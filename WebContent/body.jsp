			//Best 슬라이드 형성
			function fn_getBestGodMain(dspCtgryNo, otltYn, bestCd) {
				$
						.ajax({
							type : "GET",
							async : true,
							url : "/ui2/display/getBestGod",
							data : {
								"dspCtgryNo" : dspCtgryNo,
								"otltYn" : otltYn,
								"brndId" : "",
								"bestPageYn" : "",
								"godListSize" : 20
							},
							beforeSend : function(request) {
								var csrfToken = $("meta[name='_csrf']").attr(
										"content");
								var csrfName = $("meta[name='_csrf_header']")
										.attr("content");
								request.setRequestHeader(csrfName, csrfToken);
							},
							success : function(data) {
								getSlick("#mBest-tab-cont",
										"ulBestKwdCnrGodList", bestCd,
										"#mainCnrBestSellerSlideBtn", data)
							},
							error : function(pa_data, status, err) {
							},
							complete : function() {
							}
						});
			}

			function getSlick(tabId, godListId, cd, slideBtnId, data) {
				$(tabId + "1 #" + godListId + "1").html('');
				$(tabId + "2 #" + godListId + "2").html('');

				var html = '';

				html = '<ul id="' + godListId + cd + '"' + ' class="swiper-wrapper">';
				html += data;
				html += '</ul>';
				//console.log(html);
				$(tabId + cd + " .product-list").html(html);
				if ($("#" + godListId + cd).length > 0
						&& $("#" + godListId + cd).find("li").length > 5) {
					$(tabId + cd + " " + slideBtnId + "1").show();
					$(tabId + cd + " " + slideBtnId + "2").show();

					$(slideBtnId + '2' + ' .slide-pagination .slick-dots')
							.remove();

					var autoBtns = $('#visualSlide10_2').find(
							'.main-best-cont.on .slide-auto');
					var brandSlideWrapper = $('#visualSlide10_2').find(
							'.main-best-cont.on .swiper-wrapper');

					var brandSlideSlick = brandSlideWrapper.slick({
						draggable : true,
						autoplay : true,
						autoplaySpeed : 5000,
						infinite : true,
						slidesToShow : 5,
